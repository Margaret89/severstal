block variables
	- var title = 'Содержание';
	- 
		var pagelist = [
			{href: 'main.html',name:'Главная'},
			{href: 'stock.html',name:'Акции'},
			{href: 'stock-detail.html',name:'Акции детальная'},
			{href: 'products.html',name:'Продукты'},
			{href: 'faq.html',name:'FAQ'},
